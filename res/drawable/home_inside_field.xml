<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#74D674"
                android:endColor="#027202" />

        </shape>
    </item>
    <item
        android:left="0dp"
        android:top="0dp"
        android:bottom="1dp"
        android:right="1dp">
        <shape android:shape="oval">
            <solid android:color="#49C049" />
        </shape>
    </item>

</layer-list>

